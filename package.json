{
  "name": "start-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:app": "tsx scripts/build-apps.ts",
    "build:lib": "tsx scripts/build-libs.ts",
    "preview": "vite preview",
    "create-pkg": "tsx scripts/create-package.ts",
    "lint": "eslint --ext .ts,.vue packages",
    "fix": "eslint --fix --ext .ts,.vue packages",
    "prepare": "husky",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@rollup/plugin-babel": "^6.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.5.1",
    "fs-extra": "^11.3.2",
    "husky": "^9.1.7",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.54.0",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.3"
  },
  "husky": {
    "hooks": {
      "post-checkout": "git submodule update"
    }
  }
}
